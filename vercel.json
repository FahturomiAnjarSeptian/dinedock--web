{
  "version": 2,
  "functions": {
    "app.js": {
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "^/img/(.*)",
      "dest": "/public/img/$1"
    },
    {
      "src": "^/css/(.*)",
      "dest": "/public/css/$1"
    },
    {
      "src": "^/(.*)",
      "dest": "/app.js"
    }
  ]
}
